    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.2",
    "ts-transform-paths": "^3.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "yaml": "^2.7.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/semver": "^7.7.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
