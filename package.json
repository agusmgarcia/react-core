{
  "name": "@agusmgarcia/react-core",
  "version": "1.3.1",
  "author": "Agus Garcia",
  "description": "Set of utilities to run, lint, format and build applications and libraries",
  "scripts": {
    "prepack": "webpack --mode=production && cpy README.md CHANGELOG.md ../.. --cwd=.github",
    "postpack": "del bin README.md CHANGELOG.md"
  },
  "bin": {
    "agusmgarcia-build": "bin/build.js",
    "agusmgarcia-check": "bin/check.js",
    "agusmgarcia-format": "bin/format.js",
    "agusmgarcia-postpack": "bin/postpack.js",
    "agusmgarcia-prepack": "bin/prepack.js",
    "agusmgarcia-start": "bin/start.js"
  },
  "peerDependencies": {
    "@types/eslint": "^8.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "cpy-cli": "^5.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sort": "^3.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "next": "^14.0.0",
    "prettier": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
