{
  "name": "@agusmgarcia/react-core",
  "core": "lib",
  "version": "4.4.5",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Agus Garcia",
  "description": "Set of opinionated commands, state manager and utilities for NextJS applications, libraries and Azure functions",
  "bin": {
    "agusmgarcia-react-core-build": "bin/build.js",
    "agusmgarcia-react-core-check": "bin/check.js",
    "agusmgarcia-react-core-deploy": "bin/deploy.js",
    "agusmgarcia-react-core-format": "bin/format.js",
    "agusmgarcia-react-core-postpack": "bin/postpack.js",
    "agusmgarcia-react-core-prepack": "bin/prepack.js",
    "agusmgarcia-react-core-regenerate": "bin/regenerate.js",
    "agusmgarcia-react-core-start": "bin/start.js",
    "agusmgarcia-react-core-test": "bin/test.js"
  },
  "scripts": {
    "build": "node -r ts-node/register -r tsconfig-paths/register src/_bin/build.ts",
    "check": "node -r ts-node/register -r tsconfig-paths/register src/_bin/check.ts",
    "deploy": "node -r ts-node/register -r tsconfig-paths/register src/_bin/deploy.ts",
    "format": "node -r ts-node/register -r tsconfig-paths/register src/_bin/format.ts",
    "postpack": "node -r ts-node/register -r tsconfig-paths/register src/_bin/postpack.ts",
    "prepack": "node -r ts-node/register -r tsconfig-paths/register src/_bin/prepack.ts",
    "regenerate": "node -r ts-node/register -r tsconfig-paths/register src/_bin/regenerate.ts",
    "start": "node -r ts-node/register -r tsconfig-paths/register src/_bin/start.ts",
    "test": "node -r ts-node/register -r tsconfig-paths/register src/_bin/test.ts"
  },
  "peerDependencies": {
    "@next/env": "^15.3.2",
    "@testing-library/react": "^16.3.0",
